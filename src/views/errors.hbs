<div class="container">
  <div class="row valign-wrapper" style="width:100%;height:100%;position:absolute;">
    <div class="col s6 offset-s3 valign">
      <div class="card" style="padding:20px 10px 10px 10px">
        <div class="card-title center-align">
          {{errorTitle}}
        </div>
        <div class="row">
          <h1 class="center-align text-long-shadow col s12">{{statusCode}}</h1>
        </div>
        <div class="card-content center-align">
          {{errorMessage}}
        </div>
      </div>
    </div>
  </div>
</div>

